<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <title>{% block title %}onetoshop{% endblock %}</title>
        {% block stylesheets %}
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <!-- Bootstrap CSS
            ============================================ -->
        <link rel="stylesheet" href="css/bootstrap.min.css">
        <!-- meanmenu CSS
            ============================================ -->
        <link rel="stylesheet" href="css/meanmenu/meanmenu.min.css">
        <!-- Notika icon CSS
            ============================================ -->
        <link rel="stylesheet" href="css/notika-custom-icon.css">
        <!-- main CSS
            ============================================ -->
        <link rel="stylesheet" href="css/main.css">
        <!-- style CSS
            ============================================ -->
        <link rel="stylesheet" href="style.css">
        <!-- responsive CSS
            ============================================ -->
        <link rel="stylesheet" href="css/responsive.css">
        <!-- slider CSS
            ============================================ -->
        <link rel="stylesheet" href="ism/css/my-slider.css"/>

        <!-- flip card CSS
            ============================================ -->
            <link href='css/bootstrap.css' rel='stylesheet' />

            <link href="css/pe-icon-7-stroke.css" rel="stylesheet" />

            <link href='css/rotating-card.css' rel='stylesheet' />

            <link href="https://netdna.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet">

            <script src="https://google-code-prettify.googlecode.com/svn/loader/run_prettify.js"></script>
        {% endblock %}
    </head>
    <body>
        {% block navbar %}
        <!-- Start Header Top Area -->
        <div class="header-top-area">
            <div class="container">
                <div class="row">
                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                        <div class="logo-area">
                            <a href="{{ path('homepage')}}"><img src="img/logo/logo.png" alt="" /></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- End Header Top Area -->
        <!-- Mobile Menu start -->
        <div class="mobile-menu-area">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <div class="mobile-menu">
                            <nav id="dropdown">
                                <ul class="mobile-menu-nav">
                                    <li>
                                        <a data-toggle="collapse" data-target="#Charts" href="{{ path('homepage')}}">Home</a>
                                    </li>
                                    <li>
                                        <a data-toggle="collapse" data-target="#demoevent" href="{{ path('service')}}">service</a>
                                    </li>
                                    <li>
                                        <a data-toggle="collapse" data-target="#democrou" href="{{ path('functionaliteit')}}">functionaliteit</a>
                                    </li>
                                    <li>
                                        <a data-toggle="collapse" data-target="#demolibra" href="{{ path('homepage')}}">Apps</a>
                                    </li>
                                    <li>
                                        <a data-toggle="collapse" data-target="#demodepart" href="{{ path('homepage')}}">Blogs</a>
                                    </li>
                                    <li>
                                        <a data-toggle="collapse" data-target="#Miscellaneousmob" href="{{ path('contact')}}">Contact</a>
                                    </li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Mobile Menu end -->
        <!-- Main Menu area start-->
        <div class="main-menu-area mg-tb-40">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <ul class="nav nav-tabs notika-menu-wrap menu-it-icon-pro">
                            <li>
                                <a href="{{ path('homepage')}}"><i class="notika-icon notika-house"></i> Home</a>
                            </li>
                            <li>
                                <a href="{{ path('service')}}"><i class="notika-icon notika-bar-chart"></i> Service</a>
                            </li>
                            <li>
                                <a href="{{ path('functionaliteit')}}"><i class="notika-icon notika-edit"></i> Functionaliteit</a>
                            </li>
                            <li>
                                <a href="{{ path('homepage')}}"><i class="notika-icon notika-app"></i> Apps</a>
                            </li>
                            <li>
                                <a href="{{ path('homepage')}}"><i class="notika-icon notika-windows"></i> Blogs</a>
                            </li>
                            <li>
                                <a href="{{ path('contact')}}"><i class="notika-icon notika-mail"></i> Contact</a>
                            </li>
                        </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Main Menu area End-->
        <!-- Form Examples area start-->
                <!-- hier moet de content -->
        <!-- Form Examples area End-->
        {% endblock %}
        {% block body %}{% endblock %}
        {% block footer %}
        <!-- Start Footer area-->
            <footer>
                <div class="footer-copyright-area">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                <p>Onetoshop &copy; 2007 - 2019 | <span>Alle rechten voorbehouden</span></p>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">
                                <p class="text-right"><a href="/nl/algemene-voorwaarden" rel="noindex, nofollow">Algemene voorwaarden</a> | <a href="/nl/privacy-statement" rel="noindex, nofollow">Privacy statement</a></p>
                            </div>
                        </div>
                    </div>
                </div>
            </footer>
        {% endblock %}
        {% block javascripts %}
            <!-- jquery
		    ============================================ -->
            <script src="js/vendor/jquery-1.12.4.min.js"></script>
            <!-- bootstrap JS
                ============================================ -->
            <script src="js/bootstrap.min.js"></script>
            <!-- meanmenu JS
                ============================================ -->
            <script src="js/meanmenu/jquery.meanmenu.js"></script>
            <!-- knob JS
                ============================================ -->
            <script src="js/knob/jquery.knob.js"></script>
            <script src="js/knob/jquery.appear.js"></script>
            <script src="js/knob/knob-active.js"></script>
            <!-- main JS
                ============================================ -->
            <script src="js/main.js"></script>
            <!-- slider JS
                ============================================ -->
            <script src="ism/js/ism-2.2.min.js"></script>


            <script src="js/jquery-1.10.2-1.js" type="text/javascript"></script>
            <script src="js/bootstrap.js" type="text/javascript"></script>

            <script>
                const card = document.querySelector('.card'),
                    input = document.querySelector('.input'),
                    line2 = document.querySelector('.line2');
                document.querySelector('.form').addEventListener('submit', function(e) {
                    input.blur();
                    card.classList.add('saving');
                    e.preventDefault();
                });
                line2.addEventListener('animationend', function(e) {
                    setTimeout(() => {
                        card.classList.add('done');
                    }, 1000);
                });

            </script>
        {% endblock %}
    </body>
</html>
